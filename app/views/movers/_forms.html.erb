<%=form_for(@mover) do |f|%>
<%= f.text_field :first_name, placeholder: 'First Name' %><br><br>
<%= f.text_field :last_name, placeholder: 'Last Name' %><br><br>
<%= f.email_field :email, placeholder: 'What is your email?'%><br><br>
<%= f.number_field :age, placeholder: 'Age'%><br><br>
<%= f.text_field :occupation, placeholder: 'occupation' %><br><br>
<%= f.text_field :location, placeholder: 'Where are you located currently'%> <br>

<%= f.submit 'Create', class:'submit-btns'%>
<%end%>
